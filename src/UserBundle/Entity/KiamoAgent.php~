<?php

namespace UserBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * KiamoAgent
 *
 * @ORM\Table(name="kiamo_agent")
 * @ORM\Entity(repositoryClass="UserBundle\Repository\KiamoAgentRepository")
 */
class KiamoAgent
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @ORM\ManyToOne(targetEntity="UserBundle\Entity\Utilisateurs")
     * @ORM\JoinColumn(name="codeProfil", referencedColumnName="Code_Profil",nullable=true)
    */
    private $codeProfil;

    /**
     * @var string
     *
     * @ORM\Column(name="numeroTelephone", type="string", length=10, nullable=true)
     */
    private $numeroTelephone;

    /**
     * @var int
     *
     * @ORM\Column(name="idAgent", type="integer", nullable=true)
     */
    private $idAgent;

    /**
     * @var int
     *
     * @ORM\Column(name="idService", type="integer", nullable=true)
     */
    private $idService;
    
    /**
     * @var string
     *
     * @ORM\Column(name="Login", type="string", length=30)
     */
    private $login;


    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    

    /**
     * Set numeroTelephone
     *
     * @param string $numeroTelephone
     *
     * @return KiamoAgent
     */
    public function setNumeroTelephone($numeroTelephone)
    {
        $this->numeroTelephone = $numeroTelephone;

        return $this;
    }

    /**
     * Get numeroTelephone
     *
     * @return string
     */
    public function getNumeroTelephone()
    {
        return $this->numeroTelephone;
    }

    /**
     * Set idAgent
     *
     * @param integer $idAgent
     *
     * @return KiamoAgent
     */
    public function setIdAgent($idAgent)
    {
        $this->idAgent = $idAgent;

        return $this;
    }

    /**
     * Get idAgent
     *
     * @return int
     */
    public function getIdAgent()
    {
        return $this->idAgent;
    }

    /**
     * Set idService
     *
     * @param integer $idService
     *
     * @return KiamoAgent
     */
    public function setIdService($idService)
    {
        $this->idService = $idService;

        return $this;
    }

    /**
     * Get idService
     *
     * @return int
     */
    public function getIdService()
    {
        return $this->idService;
    }

    /**
     * Set codeProfil
     *
     * @param \UserBundle\Entity\Utilisateurs $codeProfil
     *
     * @return KiamoAgent
     */
    public function setCodeProfil(\UserBundle\Entity\Utilisateurs $codeProfil = null)
    {
        $this->codeProfil = $codeProfil;

        return $this;
    }

    /**
     * Get codeProfil
     *
     * @return \UserBundle\Entity\Utilisateurs
     */
    public function getCodeProfil()
    {
        return $this->codeProfil;
    }

    /**
     * Set login
     *
     * @param string $login
     *
     * @return KiamoAgent
     */
    public function setLogin($login)
    {
        $this->login = $login;

        return $this;
    }

    /**
     * Get login
     *
     * @return string
     */
    public function getLogin()
    {
        return $this->login;
    }
}
